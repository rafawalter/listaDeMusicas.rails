<% if notice %>
  <aside id='notice'><%= notice %></aside>
<% end %>

<h1>MÃºsicas para agora</h1>

<ul class='musicas'>
  <% cache @musicas do %>
    <% @musicas.each do |musica| %>
      <% cache musica do %>
        <li>
          <h2><%= musica.titulo %></h2>
          <div class='musico'>
            <%= musica.musico %>
          </div>
          <p class='temas'>
            <%= musica.temas %>
          </p>
        </li>
      <% end %>
    <% end %>
  <% end %>
</ul>
