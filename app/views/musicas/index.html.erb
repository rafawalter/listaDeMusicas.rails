<p id="notice"><%= notice %></p>

<h1>Musicas</h1>

<table>
  <thead>
    <tr>
      <th>Nome</th>
      <th>Autor e compositor</th>
      <th>Temas</th>
      <th>Cifras</th>
      <th>Popularidade</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @musicas.each do |musica| %>
      <tr>
        <td><%= musica.nome %></td>
        <td><%= musica.autor_e_compositor %></td>
        <td><%= musica.temas %></td>
        <td><%= musica.cifras %></td>
        <td><%= musica.popularidade %></td>
        <td><%= link_to 'Show', musica %></td>
        <td><%= link_to 'Edit', edit_musica_path(musica) %></td>
        <td><%= link_to 'Destroy', musica, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Musica', new_musica_path %>
